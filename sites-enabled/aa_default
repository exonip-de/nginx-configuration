server {
    listen 80 default_server;
    listen 443 ssl http2;
    access_log off;
    error_log off;

    ssl_certificate /etc/letsencrypt/live/exonip.de-0001/fullchain.pem;
    ssl_certificate_key /etc/letsencrypt/live/exonip.de-0001/privkey.pem;

    access_log off;
    error_log off;

    return 404;
}
